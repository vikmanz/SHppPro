<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/profileFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.vikmanz.shpppro.ui.my_profile.MyProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_my_profile">
        <action
            android:id="@+id/action_profileFragment_to_myContactsFragment"
            app:destination="@id/contactsFragment" />
        <argument
            android:name="@string/safe_arg_id"
            app:argType="com.vikmanz.shpppro.ui.base.BaseArgument"/>
    </fragment>


    <fragment
        android:id="@+id/contactsFragment"
        android:name="com.vikmanz.shpppro.ui.my_contacts.MyContactsFragment"
        android:label="fragment_my_contacts"
        tools:layout="@layout/fragment_my_contacts">
        <action
            android:id="@+id/action_myContactsFragment_to_contactDetailsFragment"
            app:destination="@id/contactDetailsFragment" />
        <argument
            android:name="@string/safe_arg_id"
            app:argType="com.vikmanz.shpppro.ui.base.BaseArgument" />
    </fragment>


    <fragment
        android:id="@+id/contactDetailsFragment"
        android:name="com.vikmanz.shpppro.ui.my_profile.MyProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_contact_profile">
        <argument
            android:name="@string/safe_arg_id"
            app:argType="com.vikmanz.shpppro.ui.base.BaseArgument" />
    </fragment>


</navigation>